{
  "version": 2,
  "builds": [
    {
      "src": "app.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "app.py"
    }
  ],
  "env": {
    "MAIL_SERVER": "smtp.gmail.com",
    "MAIL_PORT": "587",
    "MAIL_USE_TLS": "true",
    "MAIL_USERNAME": "@mail_username",
    "MAIL_PASSWORD": "@mail_password",
    "MAIL_DEFAULT_SENDER": "@mail_username",
    "GOOGLE_CREDENTIALS": "@google_credentials"
  }
}
